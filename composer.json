{
    "name": "ffi/location",
    "type": "library",
    "description": "PHP library for determining the physical location of binaries",
    "license": "MIT",
    "keywords": ["ffi", "bin", "location", "utility"],
    "support": {
        "source": "https://github.com/php-ffi/location",
        "issues": "https://github.com/php-ffi/location/issues",
        "docs": "https://github.com/php-ffi/location/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru",
            "homepage": "https://serafimarts.ru",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "FFI\\Location\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\Location\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
