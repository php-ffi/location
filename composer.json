{
    "name": "ffi/location",
    "description": "PHP library for determining the physical location of binaries",
    "license": "MIT",
    "keywords": ["ffi", "bin", "location", "utility"],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "FFI\\Location\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.9",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\Location\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
